yılbaşı_kutlanırken 		vp := vp[vp=time_conv] : \w0\t0.(!time !anonymous_celebrating_new_years);
ali_tv_izlerken 			vp := vp[vp=time_conv] : \w0\t0.(!time !ali_watching_tv);
ayşe_ödev_yaparken 			vp := vp[vp=time_conv] : \w0\t0.(!time !ayşe_doing_hw);
ahmet_yürürken 				vp := vp[vp=time_conv] : \w0\t0.(!time !ahmet_walking);
yağmur_yağarken 			vp := vp[vp=time_conv] : \w0\t0.(!time !raining);
ahmet_konuşurken 			vp := vp[vp=time_conv] : \w0\t0.(!time !ahmet_talking);
ayşe_oyun_oynarken 			vp := vp[vp=time_conv] : \w0\t0.(!time !ayşe_playing);
ben_dördüncü_ödevi_yaparken vp := vp[vp=time_conv] : \w0\t0.(!time !speaker_working_on_the_fourth_asg);
