ahmetin		n := np : !ahmet;
herkes		n := np : !everyone;
kimse		n := np : !no_one;

%%Also
de 		n := np\*np : \x.x;

%%Verbs
gül			v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!laugh x;
gel 		v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!come x;
git 		v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!go x;
dön 		v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!come_back x;
hapse_gir 	v := vp[vp=culmination,mod=false,pos=0,caus=false]\np : \x.!go_to_jail x;
ver 		v := (vp[vp=point,mod=false,pos=0,caus=false]\np)\np : \x\y.!give x y;
konuş		v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!speak x;
haber_ver	v := (vp[vp=culmination,mod=false,pos=0,caus=false]\np)\np : \y\x.!inform x y; 
söyle		v := vp[vp=process,mod=false,pos=0,caus=false]\np : \x.!tell x;
al			v := vp[vp=point,mod=false,pos=0,caus=false]\np\np : \y\x.!buy y x;
kapat		v := vp[vp=culm_proc,mod=false,pos=0,caus=false]\np\np : \y\x.!pay_off y x;
beğen		v := (vp[vp=state,mod=false,pos=0,caus=false]\np)\np : \y\x.!like y x;
duy			v := (vp[vp=point,mod=false,pos=0,caus=false]\np)\np : \y\x.!hear y x;
öğren		v := (vp[vp=culm_proc,mod=false,pos=0,caus=false]\np)\np : \y\x.!learn y x;
bekle		v := (vp[vp=process,mod=false,pos=0,caus=false]\np)\np : \y\x.!wait y x;
inan		v := (vp[vp=state,mod=false,pos=0,caus=false]\np)\np : \y\x.!believe y x;
bil			v := (vp[vp=state,mod=false,pos=0,caus=false]\np)\np : \y\x.!know y x;
gör			v := (vp[vp=culmination,mod=false,pos=0,caus=false]\np)\np : \y\x.!see y x;
izle		v := (vp[vp=process,mod=false,pos=0,caus=false]\np)\np : \y\x.!watch y x;
yakala		v := (vp[vp=culmination,mod=false,pos=0,caus=false]\np)\np : \y\x.!catch y x;

%%Auxiliary (to expand tense/aspect/modality combinations)
ol			v := vp[vp=aux,mod=?x,pos=0,caus=false]\*vp[mod=?x,pos=4]
			: \p.!AUX (p w0 t0);
ol			v := (vp[vp=aux,mod=?x,pos=0,caus=false]\np)\*(vp[mod=?x,pos=4]\np)
			: \p\x.!AUX (p x w0 t0);
ol			v := ((vp[vp=aux,mod=?x,pos=0,caus=false]\np)\np)\*((vp[mod=?x,pos=4]\np)\np)
			: \p\x\y.!AUX (p x y w0 t0);
ol			v := (((vp[vp=aux,mod=?x,pos=0,caus=false]\np)\np)\np)\*(((vp[mod=?x,pos=4]\np)\np)\np)
			: \p\x\y\z.!AUX (p x y z w0 t0);

%%Quick VPs
sen_okuldayken 				vp := vp[vp=time_conv] : \w0\t0.(!time !hearer_at_school);
